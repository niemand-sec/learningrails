<h1>Listing puerto_servicios</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @puerto_servicios.each do |puerto_servicio| %>
      <tr>
        <td><%= link_to 'Show', puerto_servicio %></td>
        <td><%= link_to 'Edit', edit_puerto_servicio_path(puerto_servicio) %></td>
        <td><%= link_to 'Destroy', puerto_servicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Puerto servicio', new_puerto_servicio_path %>
