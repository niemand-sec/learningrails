<h1>Listing tipo_de_servicios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descipcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_de_servicios.each do |tipo_de_servicio| %>
      <tr>
        <td><%= tipo_de_servicio.nombre %></td>
        <td><%= tipo_de_servicio.descipcion %></td>
        <td><%= link_to 'Show', tipo_de_servicio %></td>
        <td><%= link_to 'Edit', edit_tipo_de_servicio_path(tipo_de_servicio) %></td>
        <td><%= link_to 'Destroy', tipo_de_servicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo de servicio', new_tipo_de_servicio_path %>
