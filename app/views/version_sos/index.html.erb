<h1>Listing version_sos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @version_sos.each do |version_so| %>
      <tr>
        <td><%= version_so.nombre %></td>
        <td><%= version_so.descripcion %></td>
        <td><%= link_to 'Show', version_so %></td>
        <td><%= link_to 'Edit', edit_version_so_path(version_so) %></td>
        <td><%= link_to 'Destroy', version_so, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Version so', new_version_so_path %>
